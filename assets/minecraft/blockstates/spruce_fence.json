{
  "multipart": [
    {
      "apply": {
        "model": "minecraft:block/spruce_fence_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "uvlock": true,
        "model": "minecraft:block/spruce_fence_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "uvlock": true,
        "y": 90,
        "model": "minecraft:block/spruce_fence_side"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "uvlock": true,
        "y": 180,
        "model": "minecraft:block/spruce_fence_side"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "uvlock": true,
        "y": 270,
        "model": "minecraft:block/spruce_fence_side"
      }
    }
  ]
}